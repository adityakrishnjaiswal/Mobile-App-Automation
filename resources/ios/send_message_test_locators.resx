  <!-- Message Input Field -->
  <data name="messageInput" xml:space="preserve">
    <value>//android.widget.EditText[contains(@resource-id, 'messageInput')]</value>
  </data>

  <!-- Send Button -->
  <data name="sendButton" xml:space="preserve">
    <value>//android.widget.Button[contains(@text, 'Send')]</value>
  </data>

