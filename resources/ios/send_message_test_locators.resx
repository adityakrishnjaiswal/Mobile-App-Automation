  </data>

  <!-- Error Message for General Exception -->
  <data name="generalError" xml:space="preserve">
    <value>Test failed due to an unexpected error: {0}</value>
  </data>

  <!-- Message Input Placeholder Text -->
  <data name="messageInputPlaceholder" xml:space="preserve">
    <value>Type your message here...</value>
  </data>

  <!-- Success Message Text -->
  <data name="testSuccessMessage" xml:space="preserve">
    <value>Test passed: Message sent and received successfully!</value>
